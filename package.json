{
  "name": "obstec",
  "version": "1.0.0",
  "description": "Система учета вычислительной техники на express и angular с подключением MongoDB",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client-install": "npm install --prefix client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/010MochalovPA/ObsTec.git"
  },
  "keywords": [
    "express",
    "angular",
    "nodejs",
    "mongodb"
  ],
  "author": "Pavel Mochalov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/010MochalovPA/ObsTec/issues"
  },
  "homepage": "https://github.com/010MochalovPA/ObsTec#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.2",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^6.5.1",
    "nodemon": "^2.0.15"
  }
